{
  "proxyMode": "openrouter",
  "anthropic": {
    "baseUrl": "https://api.anthropic.com",
    "timeout": 120000
  },
  "openrouter": {
    "baseUrl": "https://openrouter.ai/api",
    "defaultModel": "anthropic/claude-3.5-haiku",
    "modelMapping": {
      "sonnet": "z-ai/glm-4.5",
      "opus": "moonshotai/kimi-k2-0905",
      "haiku": "google/gemini-2.5-flash"
    }
  },
  "server": {
    "host": "0.0.0.0",
    "port": 8082,
    "logLevel": "INFO"
  },
  "monitoring": {
    "enabled": true,
    "maxRequests": 1000,
    "retentionHours": 24
  }
}