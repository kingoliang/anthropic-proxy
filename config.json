{
  "proxyMode": "openrouter",
  "anthropic": {
    "baseUrl": "https://api.anthropic.com",
    "timeout": 120000
  },
  "openrouter": {
    "baseUrl": "https://openrouter.ai/api",
    "defaultModel": "anthropic/claude-3.5-haiku",
    "modelMapping": {
      "sonnet": "openai/gpt-5",
      "opus": "openai/gpt-5",
      "haiku": "anthropic/claude-3.5-haiku"
    }
  },
  "server": {
    "host": "0.0.0.0",
    "port": 8082,
    "logLevel": "INFO"
  },
  "monitoring": {
    "enabled": true,
    "maxRequests": 1000,
    "retentionHours": 24
  }
}